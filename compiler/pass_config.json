{
  "pipeline": [
    "dce",
    "loop-unroll",
    "simplify",
    "dce",
    "cse",
    "load-elim",
    "dse",
    "tree-level-cache",
    "simplify",
    "dce",
    "slp-vectorization",
    "dce",
    "cse",
    "mad-synthesis",
    "dce",
    "hir-to-lir",
    "copy-propagation",
    "lir-dce",
    "simplify-cfg",
    "phi-elimination",
    "inst-scheduling",
    "mir-reg-pressure-profiler",
    "mir-register-allocation",
    "mir-to-vliw"
  ],
  "passes": {
    "simplify": {
      "enabled": true,
      "options": {
        "constant_folding": true,
        "identities": true,
        "canonicalization": true,
        "select_to_mul": true,
        "select_to_alu_mux": false,
        "parity_pattern": true,
        "add_mul_fold": true
      }
    },
    "load-elim": {
      "enabled": true,
      "options": {
        "restrict_ptr": true
      }
    },
    "dse": {
      "enabled": true,
      "options": {
        "restrict_ptr": true
      }
    },
    "tree-level-cache": {
      "enabled": true,
      "options": {
        "levels": 3,
        "estimate_ops": true
      }
    },
    "slp-vectorization": {
      "enabled": true,
      "options": {
        "restrict_ptr": true,
        "vectorize_memory": true,
        "gather": true,
        "vectorize_alu": true
      }
    },
    "simplify-cfg": {
      "enabled": true,
      "options": {
        "remove_unreachable": true,
        "thread_trampolines": true,
        "merge_blocks": true,
        "simplify_cond_jump": true
      }
    },
    "inst-scheduling": {
      "enabled": true,
      "options": {
        "prefer_load_fill": false,
        "devectorize_valu_to_alu": true,
        "devectorize_vector_ops_to_alu": true,
        "devectorize_vbroadcast_to_alu": true,
        "devectorize_partial_alu_fill": true,
        "prioritize_load_unblock": true,
        "register_pressure_limit": 0
      }
    },
    "mir-reg-pressure-profiler": {
      "enabled": false,
      "options": {}
    },
    "mir-register-allocation": {
      "enabled": true,
      "options": {
        "enable_spilling": false,
        "spill_infra_size": 35,
        "debug_spills": false
      }
    }
  }
}
